      "output": { "outputOnFailure": true }
    },
    {
      "name": "f",
      "configurePreset": "f",
      "output": { "outputOnFailure": true }
    },
    {
      "name": "g",
      "configurePreset": "g",
      "output": { "outputOnFailure": true }
    },
    {
      "name": "h",
