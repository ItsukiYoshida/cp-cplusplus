{
  "version": 3,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 21,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "default",
      "hidden": true,
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build/${presetName}",
	  "cacheVariables": {
        "TARGET_PROBLEM": "${presetName}"
      }
    },
    { "name": "a", "inherits": "default" },
    { "name": "b", "inherits": "default" },
    { "name": "c", "inherits": "default" },
    { "name": "d", "inherits": "default" },
    { "name": "e", "inherits": "default" },
    { "name": "f", "inherits": "default" },
    { "name": "g", "inherits": "default" },
    { "name": "h", "inherits": "default" },
    { "name": "i", "inherits": "default" }
  ],
  "buildPresets": [
    { "name": "a", "configurePreset": "a" },
    { "name": "b", "configurePreset": "b" },
    { "name": "c", "configurePreset": "c" },
    { "name": "d", "configurePreset": "d" },
    { "name": "e", "configurePreset": "e" },
    { "name": "f", "configurePreset": "f" },
    { "name": "g", "configurePreset": "g" },
    { "name": "h", "configurePreset": "h" },
    { "name": "i", "configurePreset": "i" }
  ],
  "testPresets": [
    {
      "name": "a",
